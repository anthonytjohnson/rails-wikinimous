<%= form_for(@article) do |f| %>
  <div class="col-sm-8 col-sm-offset-2">
    <div class="form-group">
      <%= f.label :title %>
      <%= f.text_field :title %>
    </div>
      <div class="form-group">
      <%= f.label :content %>
      <%= f.text_field :content %>
    </div>
    <%= f.submit %>
  </div>

<% end %>
